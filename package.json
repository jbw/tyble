{
    "name": "tyble",
    "version": "0.2.0",
    "description": "A typesafe React table component",
    "main": "dist/tyble.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "./node_modules/.bin/webpack",
        "build:prod": "./node_modules/.bin/webpack -p",
        "start": "./node_modules/.bin/webpack-dev-server --progress --inline --hot",
        "lint:css": "./node_modules/.bin/stylelint ./src/**/*.tsx",
        "lint:ts": "./node_modules/.bin/tslint -p tsconfig.json -c tslint.json -t stylish",
        "prepublishOnly": "./node_modules/.bin/webpack -p",
        "test": "./node_modules/.bin/jest",
        "clean-install": "rm -rf node_modules/ && yarn cache clean && yarn install --pure-lockfile"
    },
    "files": [
        "dist/"
    ],
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "identity-obj-proxy"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ]
    },
    "pre-commit": "pass",
    "repository": "git+https://github.com/jbw/tyble.git",
    "author": "Jason B Watson",
    "license": "MIT",
    "devDependencies": {
        "@types/enzyme": "^3.1.4",
        "@types/react": "^16.0.25",
        "@types/react-dom": "^16.0.3",
        "awesome-typescript-loader": "^3.2.3",
        "css-loader": "^0.28.7",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^21.2.1",
        "node-sass": "^4.6.1",
        "react-axe": "^2.1.7",
        "react-test-renderer": "^16.1.0",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "stylelint": "^8.2.0",
        "stylelint-config-standard": "^17.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.0.0",
        "ts-jest": "^21.1.4",
        "tslint": "^5.8.0",
        "tslint-microsoft-contrib": "^5.0.1",
        "tslint-react": "^3.2.0",
        "typescript": "^2.6.1",
        "webpack": "^3.7.1",
        "webpack-bundle-analyzer": "^2.9.1",
        "webpack-dev-server": "^2.9.2"
    },
    "dependencies": {
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "styled-components": "^2.2.3"
    }
}
